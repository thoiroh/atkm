{
  "/api/*": {
    "target": "http://nginx:80",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Accept": "application/json",
      "Content-Type": "application/json"
    }
  },
  "/uploads/*": {
    "target": "http://nginx:80",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  }
}
