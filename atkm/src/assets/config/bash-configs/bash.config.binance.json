{
  "id": "binance-debug-json",
  "title": "bbb Binance API Debug Terminal - JSON Config",
  "subtitle": "Configuration JSON pour tests et d√©veloppements futurs",
  "defaultEndpoint": "account",
  "terminal": {
    "editable": true,
    "height": "480px",
    "showControls": true,
    "customCommands": [
      {
        "name": "clear",
        "description": "Clear terminal logs"
      },
      {
        "name": "help",
        "description": "Show available commands"
      },
      {
        "name": "symbol",
        "description": "Set trading symbol (e.g., symbol BTCUSDT)"
      }
    ]
  },
  "table": {
    "showEmptyState": true,
    "itemsPerPage": 100,
    "searchEnabled": false
  },
  "endpoints": [
    {
      "id": "account",
      "name": "Account Information",
      "url": "http://localhost:8000/api/v3/account",
      "method": "GET",
      "headers": {
        "Content-Type": "application/json",
        "X-Debug": "true"
      },
      "cacheable": true,
      "cacheDuration": 30000,
      "columns": [
        {
          "key": "assets",
          "label": "Asset",
          "width": "10%",
          "align": "left",
          "type": "text",
          "sortable": true,
          "cssClass": "font-weight-bold asset-cell"
        },
        {
          "key": "free",
          "label": "Available",
          "width": "40%",
          "align": "right",
          "type": "custom",
          "sortable": true
        },
        {
          "key": "locked",
          "label": "Locked",
          "width": "10%",
          "align": "right",
          "type": "custom",
          "sortable": true,
          "cssClass": "locked-balance"
        },
        {
          "key": "total",
          "label": "Total Balance",
          "width": "10%",
          "align": "right",
          "type": "custom",
          "sortable": true,
          "cssClass": "total-balance font-weight-bold"
        },
        {
          "key": "usdValue",
          "label": "USD",
          "width": "5%",
          "align": "right",
          "type": "currency",
          "sortable": true,
          "visible": false
        }
      ]
    },
    {
      "id": "trades",
      "name": "Trade History",
      "url": "http://localhost:8000/api/v3/myTrades",
      "method": "GET",
      "params": {
        "symbol": "BTCUSDT",
        "limit": 100
      },
      "cacheable": true,
      "cacheDuration": 60000,
      "columns": [
        {
          "key": "symbol",
          "label": "Symbol",
          "width": "12%",
          "align": "left",
          "type": "badge",
          "sortable": true
        },
        {
          "key": "side",
          "label": "Side",
          "width": "8%",
          "align": "center",
          "type": "badge",
          "cssClass": "trade-side"
        },
        {
          "key": "price",
          "label": "Price",
          "width": "16%",
          "align": "right",
          "type": "custom",
          "sortable": true
        },
        {
          "key": "qty",
          "label": "Quantity",
          "width": "16%",
          "align": "right",
          "type": "custom",
          "sortable": true
        },
        {
          "key": "quoteQty",
          "label": "Quote Qty",
          "width": "16%",
          "align": "right",
          "type": "custom",
          "sortable": true
        },
        {
          "key": "commission",
          "label": "Fee",
          "width": "10%",
          "align": "right",
          "type": "custom"
        },
        {
          "key": "time",
          "label": "Date",
          "width": "16%",
          "align": "center",
          "type": "date",
          "sortable": true
        },
        {
          "key": "isMaker",
          "label": "Maker",
          "width": "6%",
          "align": "center",
          "type": "boolean",
          "visible": false
        }
      ]
    },
    {
      "id": "orders",
      "name": "Order History",
      "url": "http://localhost:8000/api/v3/allOrders",
      "method": "GET",
      "params": {
        "symbol": "BTCUSDT",
        "limit": 100
      },
      "cacheable": true,
      "cacheDuration": 45000,
      "columns": [
        {
          "key": "symbol",
          "label": "Symbol",
          "width": "12%",
          "align": "left",
          "type": "text",
          "sortable": true
        },
        {
          "key": "side",
          "label": "Side",
          "width": "8%",
          "align": "center",
          "type": "badge",
          "cssClass": "order-side"
        },
        {
          "key": "type",
          "label": "Type",
          "width": "10%",
          "align": "center",
          "type": "text"
        },
        {
          "key": "origQty",
          "label": "Quantity",
          "width": "14%",
          "align": "right",
          "type": "custom"
        },
        {
          "key": "executedQty",
          "label": "Executed",
          "width": "14%",
          "align": "right",
          "type": "custom"
        },
        {
          "key": "price",
          "label": "Price",
          "width": "14%",
          "align": "right",
          "type": "custom"
        },
        {
          "key": "status",
          "label": "Status",
          "width": "10%",
          "align": "center",
          "type": "badge",
          "cssClass": "order-status"
        },
        {
          "key": "time",
          "label": "Created",
          "width": "18%",
          "align": "center",
          "type": "date",
          "sortable": true
        }
      ]
    },
    {
      "id": "ticker",
      "name": "Ticker Prices",
      "url": "http://localhost:8000/api/v3/ticker/price",
      "method": "GET",
      "cacheable": true,
      "cacheDuration": 10000,
      "columns": [
        {
          "key": "symbol",
          "label": "Symbol",
          "width": "40%",
          "align": "left",
          "type": "text",
          "sortable": true
        },
        {
          "key": "price",
          "label": "Price",
          "width": "30%",
          "align": "right",
          "type": "custom",
          "sortable": true
        },
        {
          "key": "priceChangePercent",
          "label": "24h Change %",
          "width": "30%",
          "align": "right",
          "type": "percentage",
          "sortable": true,
          "cssClass": "price-change-cell"
        }
      ]
    }
  ],
  "actions": [
    {
      "id": "refresh-all",
      "label": "Refresh All",
      "icon": "refresh-cw",
      "variant": "primary"
    },
    {
      "id": "export-csv",
      "label": "Export CSV",
      "icon": "download",
      "variant": "secondary"
    },
    {
      "id": "clear-cache",
      "label": "Clear Cache",
      "icon": "trash-2",
      "variant": "warning"
    }
  ]
}
